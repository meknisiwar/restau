# Configuration de production pour hébergement
APP_ENV=prod
APP_DEBUG=0

# Clé secrète - À REMPLACER par une vraie clé sécurisée
APP_SECRET=CHANGE_ME_IN_PRODUCTION_TO_32_CHAR_SECRET

# Base de données - Sera automatiquement configurée par Railway/Render
DATABASE_URL=postgresql://username:password@localhost/restau_prod

# Configuration email (optionnel)
MAILER_DSN=null://null

# Configuration pour Railway/Render
PORT=8000

# Configuration de cache (Redis si disponible)
REDIS_URL=redis://localhost

# Configuration de log pour production
LOG_LEVEL=error
